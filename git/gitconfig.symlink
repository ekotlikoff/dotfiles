# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[core]
        excludesfile = ~/.gitignore
        editor = vim
[mergetool]
        keepBackup = false
[difftool]
        prompt = false
[help]
        autocorrect = 1
[push]
        # See `git help config` (search for push.default)
        # for more information on different options of the below setting.
        default = simple
[credential]
	helper = osxkeychain
[init]
	defaultBranch = main
